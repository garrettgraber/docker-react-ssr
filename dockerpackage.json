{
  "name": "docker-react-webpack",
  "version": "0.1.0",
  "private": false,
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "commonjs": true,
      "es6": true,
      "jquery": true
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "scripts": {
    "start": "nodemon --watch server.js index.js",
    "start:dev":"webpack-dev-server --config webpack.config.babel.js  --port 8082",
    "pack": "webpack --config webpack.config.babel.js",
    "babel": "babel src -d dist",
    "build": "npm run pack && npm run babel",
    "build:webpack":"webpack --config webpack.config.babel.js"
  },
  "dependencies": {
    "ajv": "^6.5.4",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "express": "^4.16.4",
    "ip": "^1.1.5",
    "query-string": "^6.2.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.11",
    "react-redux": "^5.1.0",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "reselect": "^4.0.0",
    "uglify-es": "^3.3.9",
    "url-loader": "^1.1.2",
    "urlencode": "^1.1.0",
    "uuid": "^3.3.2"
